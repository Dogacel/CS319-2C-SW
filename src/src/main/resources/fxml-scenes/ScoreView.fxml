   <children>
      <VBox fx:id="scoreVBox" alignment="CENTER" layoutX="160.0" layoutY="130.0" prefHeight="700.0" prefWidth="963.0">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
      <Button fx:id="mainMenuButton" layoutX="540.0" layoutY="870.0" mnemonicParsing="false" onMouseClicked="#returnMenuButtonClicked" prefWidth="200.0" text="Return Main Menu" />
